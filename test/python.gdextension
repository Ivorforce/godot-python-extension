[configuration]

compatibility_minimum = 4.2
entry_symbol = "python_extension_init"

[libraries]

macos.debug = "res://bin/libgodot-python.macos.template_debug.framework"
macos.release = "res://bin/libgodot-python.macos.template_release.framework"
windows.debug.x86_32 = "res://bin/libgodot-python.windows.template_debug.x86_32.dll"
windows.release.x86_32 = "res://bin/libgodot-python.windows.template_release.x86_32.dll"
windows.debug.x86_64 = "res://bin/libgodot-python.windows.template_debug.x86_64.dll"
windows.release.x86_64 = "res://bin/libgodot-python.windows.template_release.x86_64.dll"

#linux.x86_64 = "res://bin/libgodot-python.linux.template_debug.x86_64.so"
#linux.x86_64 = "res://bin/libgodot-python.linux.template_release.x86_64.so"

linux.debug.x86_64 = "res://bin/libgodot-python.linux.template_debug.x86_64.so"
linux.release.x86_64 = "res://bin/libgodot-python.linux.template_release.x86_64.so"

#linuxbsd.debug.x86_64 = "res://bin/libgodot-python.linux.template_debug.x86_64.so"
